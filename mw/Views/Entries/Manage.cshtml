@model IEnumerable<mw.Models.Entry>

@{
    ViewBag.Title = "Manage entries";
}
<h2 id="page-title">Manage entries</h2>
<table class="table">
    @foreach (var entry in Model) {
        <tr>
            <td>@Html.ActionLink(@entry.Title, "Edit", new { id = entry.Id })</td>
            <td>@entry.Body.Substring(0, 30)</td>
            <td>@entry.DateCreated</td>
            <td>@entry.DateModified</td>
        </tr>
    }
</table>
