@model IEnumerable<mw.Models.Project>

@{
    ViewBag.Title = "Manage";
}

<h2 id="page-title">Manage</h2>
<table class="table">
    @foreach (var project in Model) {
        <tr>
            <td>@Html.ActionLink(project.Name, "Edit", new { id = project.Id })</td>
            <td>@project.Description</td>
            <td>@project.DateCreated</td>
            <td>@project.DateModified</td>
        </tr>
    }
</table>

@Html.ActionLink("Create new project", "Create")